{
    "name": "bjornjohansen/www.bjornjohansen.com",
    "description": "The www.bjornjohansen.com website",
    "license": "GPL-2.0-or-later",
    "repositories": {
        "wpackagist": {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    },
    "require": {
        "composer/installers": "^1.11",
        "roots/wordpress": "^5.7",
        "wpackagist-plugin/disable-emojis": "^1.7",
        "wpackagist-plugin/akismet": "^4.1",
        "wpackagist-plugin/oembed-gist": "^4.9",
        "wpackagist-plugin/show-parent-comment": "^0.2.1",
        "wpackagist-plugin/wp-cron-control": "^0.7.1",
        "wpackagist-plugin/wp-crontrol": "^1.10",
        "wpackagist-plugin/wordpress-seo": "^16.5",
        "wpackagist-plugin/instant-page": "^5.1",
        "wpackagist-plugin/google-site-kit": "^1.34",
        "wpackagist-theme/twentytwenty": "^1.0"
    },
    "extra": {
        "wordpress-install-dir": "wp",
        "installer-paths": {
            "content/mu-plugins/{$name}/": [
                "type:wordpress-muplugin"
            ],
            "content/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "content/themes/{$name}/": [
                "type:wordpress-theme"
            ]
        }
    }
}
