{
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
            "type": "composer",
            "url": "https://packages.dekodes.no/"
        }
    ],
    "require": {
        "composer/installers": "*",
        "johnpbloch/wordpress": "*",
        "wpackagist-plugin/disable-emojis": "*",
        "wpackagist-plugin/akismet": "*",
        "wpackagist-plugin/bj-lazy-load": "*",
        "wpackagist-plugin/duo-wordpress": "*",
        "wpackagist-plugin/epoch": "*",
        "wpackagist-plugin/ewww-image-optimizer": "*",
        "wpackagist-plugin/google-analytics-dashboard-for-wp": "*",
        "wpackagist-plugin/oembed-gist": "*",
        "wpackagist-plugin/public-post-preview": "*",
        "wpackagist-plugin/public-post-preview-configurator": "*",
        "wpackagist-plugin/show-parent-comment": "*",
        "wpackagist-plugin/simple-comment-editing": "*",
        "wpackagist-plugin/simple-history": "*",
        "wpackagist-plugin/slash-edit": "*",
        "wpackagist-plugin/dvk-social-sharing": "*",
        "wpackagist-plugin/tuxedo-big-file-uploads": "*",
        "wpackagist-plugin/user-switching": "*",
        "wpackagist-plugin/wp-cron-control": "*",
        "wpackagist-plugin/wp-crontrol": "*",
        "wpackagist-plugin/gutenberg": "*",
        "dekode/wordpress-seo-premium": "*",
        "wpackagist-plugin/goodbye-captcha": "*"
    },
    "extra": {
        "wordpress-install-dir": "wp",
        "installer-paths": {
            "content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
            "content/plugins/{$name}/": ["type:wordpress-plugin"],
            "content/themes/{$name}/": ["type:wordpress-theme" ]
        }
    },
    "require-dev": {
        "bjornjohansen/wp-pre-commit-hook": "*"
    }
}
