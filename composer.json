{
    "repositories": {
        "wpackagist": {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        "my-yoast": {
            "type": "composer",
            "url": "https://my.yoast.com/packages/"
        }
    },
    "require": {
        "composer/installers": "*",
        "johnpbloch/wordpress": "*",
        "bjornjohansen/spiderbear": "*",
        "wpackagist-plugin/disable-emojis": "*",
        "wpackagist-plugin/akismet": "*",
        "wpackagist-plugin/duo-wordpress": "*",
        "wpackagist-plugin/epoch": "*",
        "wpackagist-plugin/ewww-image-optimizer": "*",
        "wpackagist-plugin/oembed-gist": "*",
        "wpackagist-plugin/show-parent-comment": "*",
        "wpackagist-plugin/simple-comment-editing": "*",
        "wpackagist-plugin/simple-history": "*",
        "wpackagist-plugin/tuxedo-big-file-uploads": "*",
        "wpackagist-plugin/user-switching": "*",
        "wpackagist-plugin/wp-cron-control": "*",
        "wpackagist-plugin/wp-crontrol": "*",
        "wpackagist-plugin/gutenberg": "*",
        "yoast/wordpress-seo-premium": "*",
        "10up/elasticpress": "^3",
        "wpackagist-theme/hello-elementor": "*",
        "wpackagist-plugin/elementor": "*"
    },
    "extra": {
        "wordpress-install-dir": "wp",
        "installer-paths": {
            "content/mu-plugins/{$name}/": [
                "type:wordpress-muplugin"
            ],
            "content/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "content/themes/{$name}/": [
                "type:wordpress-theme"
            ]
        }
    },
    "require-dev": {
        "bjornjohansen/wp-pre-commit-hook": "*"
    }
}
